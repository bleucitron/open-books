{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
      "@stores": ["./src/stores"],
      "@stores/*": ["./src/stores/*"],
      "@interfaces": ["./src/interfaces"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@docs": ["./src/docs"],
      "@docs/*": ["./src/docs/*"]
    }
  }
}
