{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "target": "es2019",
    "module": "esnext",
    "noImplicitAny": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "paths": {
      "$lib": ["./src/lib"],
      "$lib/*": ["./src/lib/*"],
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
      "@stores": ["./src/stores"],
      "@stores/*": ["./src/stores/*"],
      "@interfaces": ["./src/interfaces"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@docs": ["./src/docs"],
      "@docs/*": ["./src/docs/*"]
    }
  },
  "include": [
    "./src/**/*.d.ts",
    "./src/**/*.js",
    "./src/**/*.ts",
    "./src/**/*.svelte"
  ]
}
